<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.aroby.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddNumbersServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.aroby.servlet.AddNumbersServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MultiplyNumbersServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.aroby.servlet.MultiplyNumbersServlet</servlet-class>
  </servlet><servlet>
    <servlet-name>GuessingGameServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.aroby.servlet.GuessingGameServlet</servlet-class>
  </servlet><servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.aroby.servlet.LoginServlet</servlet-class>
  </servlet><servlet>
  <servlet-name>ReviewServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.aroby.servlet.ReviewServlet</servlet-class>
  </servlet><servlet>
  	<servlet-name>CreateAccountServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.aroby.servlet.CreateAccountServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>SearchServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.aroby.servlet.SearchServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>TedTalkServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.aroby.servlet.TedTalkServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>AboutServlet</servlet-name>
  <servlet-class>edu.ycp.cs320.aroby.servlet.AboutServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>TedTalkViewServlet</servlet-name>
  <servlet-class>edu.ycp.cs320.aroby.servlet.TedTalkViewServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>LogoutServlet</servlet-name>
  <servlet-class>edu.ycp.cs320.aroby.servlet.LogoutServlet</servlet-class>
  </servlet>
  


  <!-- Servlet mappings -->
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddNumbersServlet</servlet-name>
    <url-pattern>/addNumbers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MultiplyNumbersServlet</servlet-name>
  	<url-pattern>/multiplyNumbers</url-pattern>
  </servlet-mapping><servlet-mapping>
    <servlet-name>GuessingGameServlet</servlet-name>
    <url-pattern>/guessingGame</url-pattern>
  </servlet-mapping><servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
   </servlet-mapping><servlet-mapping>
  	<servlet-name>CreateAccountServlet</servlet-name>
  	<url-pattern>/accountCreation</url-pattern>
   </servlet-mapping><servlet-mapping>
  	<servlet-name>ReviewServlet</servlet-name>
  	<url-pattern>/reviewPage</url-pattern>
   </servlet-mapping><servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/searchPage</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
    <servlet-name>TedTalkServlet</servlet-name>
    <url-pattern>/tedTalkPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AboutServlet</servlet-name>
    <url-pattern>/about</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TedTalkViewServlet</servlet-name>
    <url-pattern>/tedTalkView</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

  
  <welcome-file-list>
      <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>

  
  
  





